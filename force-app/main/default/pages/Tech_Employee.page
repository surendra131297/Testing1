<apex:page controller="search_TechAss" renderAs="{!if($CurrentPage.parameters.isPdf == null, null, 'pdf')}" id="myPage">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection columns="2">
                <!--<apex:inputText value="{!searchbox}"label="Search"/>-->
                <apex:inputText value="{!searchbox}" label="Search" />
                <apex:commandButton value="Search" action="{!getTech}"/>
            </apex:pageBlockSection>
            <apex:pageblockTable value="{!result}" var="a">
                <apex:column Value="{!a.Emp_Id__c}"/>
                <apex:column value="{!a.Emp_Name__c}"/>
                <apex:column value="{!a.Email__c}"/>
                <apex:column value="{!a.Completion_Date__c}"/>
                <apex:column value="{!a.Assesment_Year__c}"/>
                <apex:column value="{!a.Status__c}"/>
                <apex:column value="{!a.Comments1__c}" />
            </apex:pageblockTable>
            <apex:commandLink action="/apex/Tech_Employee?isPdf=true" target="_blank" style="text-decoration:none">
                <apex:commandButton value="Generate Pdf"/>
            </apex:commandLink>
            
        </apex:pageBlock>
    </apex:form>
</apex:page>